    class ProgramConfig
    {
        private const string version = "1.0";
        private const string developerName = "Strelkov Andrey";
        public static void ShowProgramInfo()
        {
            Console.WriteLine("Версия программы: " + version + ", разработчик: " + developerName);
        }
    }
